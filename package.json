{
  "name": "vite-ts-boilerplate",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:staging": "vue-tsc --noEmit && vite build --mode staging",
    "preview": "vite preview",
    "types-check": "vue-tsc --noEmit",
    "lint:script": "eslint -c .eslintrc.js \"./src/**/*.{js,ts,vue}\" --ignore-path .gitignore .",
    "lint:prettier": "prettier \"src/**/*+(js|ts|vue|scss)\" --ignore-path .gitignore",
    "lint:style": "stylelint src/**/*.{css,scss} -i .gitignore -i .stylelintignore",
    "lint": "npm run lint:script && npm run lint:prettier -- --check && npm run lint:style",
    "lintfix": "npm run lint:prettier -- --write && npm run lint:script -- --fix && npm run lint:style -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.20",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "pinia": "^2.1.4",
    "tailwindcss": "^3.3.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "less": "^4.1.2",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.26",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.0",
    "sass": "^1.63.6",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vite-plugin-imp": "^2.1.8",
    "vue-tsc": "^1.8.5"
  },
  "lint-staged": {
    "*.{js,ts,vue,css,scss}": "npm run lint"
  }
}
